    "openveo",
    "veo-labs",
    "cms",
    "plugins",
    "web-service",
    "angularjs",
    "bootstrap",
    "express",
    "mean",
    "mongodb",
    "oauth",
    "back-end",
    "front-end",
    "user",
    "framework",
    "authentication"
  ],
  "homepage": "https://github.com/veo-labs/openveo-core",
  "bugs": {
    "url": "https://github.com/veo-labs/openveo-core/issues"
  },
  "license": "AGPL-3.0",
  "author": "Veo-Labs <info@veo-labs.com> (http://www.veo-labs.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/veo-labs/openveo-core.git"
  },
  "dependencies": {
    "@openveo/api": "^7.0.0",
    "angular": "1.5.11",
    "angular-animate": "1.5.11",
    "angular-cookies": "1.5.11",
    "angular-formly": "8.4.1",
    "angular-formly-templates-bootstrap": "6.3.2",
    "angular-i18n": "1.5.11",
    "angular-route": "1.5.11",
    "angular-sanitize": "1.5.11",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-tinymce": "0.0.19",
    "angular-ui-tree": "2.14.0",
    "api-check": "^7.5.5",
    "async": "^3.2.0",
    "body-parser": "^1.19.0",
    "bootstrap-sass": "3.3.7",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fastclick": "^1.0.6",
    "mustache": "^4.2.0",
    "nanoid": "^3.1.23",
    "ng-file-upload": "^12.2.13",
    "ng-tasty": "0.6.1",
    "nopt": "^5.0.0",
    "oauth20-provider": "^0.6.0",
    "passport": "^0.4.1",
    "semver": "^7.3.5",
    "serve-favicon": "^2.5.0",
    "socket.io-client": "^4.1.2",
    "tinymce": "^5.9.1"
  },
  "devDependencies": {
    "@openveo/rest-nodejs-client": "^4.0.0",
    "@openveo/test": "^8.0.0",
    "angular-mocks": "1.5.11",
    "cas-server-mock": "^3.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
    "grunt": "^1.1.0",
    "grunt-cli": "^1.3.2",
    "grunt-exec": "^3.0.0",
    "grunt-gh-pages": "^3.1.0",
    "grunt-protractor-runner": "^5.0.0",
    "jsdoc": "^3.6.7",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "ldap-server-mock": "^3.0.0",
    "mocha": "^9.1.1",
    "mock-require": "^3.0.3",
    "pre-commit": "^1.2.2",
    "uglify-js": "^3.14.2"
  },
  "scripts": {
    "build": "./scripts/build.js production",
    "build:development": "./scripts/build.js",
    "build:scss": "compass compile -c ./compass.rb -e production -s compressed --force --no-sourcemap",
    "build:scss-development": "compass compile -c ./compass.rb --force",
    "doc": "npm doc:openveo ; npm run doc:back-office ; npm run doc:server",
    "doc:back-office": "npx jsdoc -c jsdoc-back-office.js -d \"./site/$(echo $npm_package_version)/api/client-back-end\" -p -r",
    "doc:clean": "./scripts/remove ./site",
    "doc:openveo": "mkdocs build -c -d \"./site/$(echo $npm_package_version)\"",
    "doc:server": "npx jsdoc -c jsdoc-server.js -d \"./site/$(echo $npm_package_version)/api/server\" -p -r",
    "e2e": "npx grunt test-e2e",
    "lint": "npx eslint \"*.js\" \"app/**/*.js\" \"migrations/**/*.js\" \"tasks/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.js\"",
    "postinstall": "node install.js",
    "postpublish": "npm run doc && npx grunt gh-pages:doc",
    "prepack": "npm run build",
    "test": "npm run test:client && npm run test:server",
    "test:client": "npx karma start ./tests/client/karmaConf.js",
    "test:server": "npx mocha --config tests/server/.mocharc.json",
    "watch": "npm run build ; npx ovWatch -d ./app/client/admin -d ./assets/be/views -c build",
    "web-driver:upgrade": "npx webdriver-manager update"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": ">=16.3.0",
    "npm": ">=7.15.1"
  }
}
